#include <stdio.h>
#include <string.h>


int main(){
	char nome[30];
	char sobrenome[30];
	printf("Informe o seu primeiro nome: ");
	gets(nome);
	printf("Informe seu ultimo sobrenome: ");
	gets(sobrenome);
	//Concatenação de duas strings (juntar)
    strcat(nome, " ");
    strcat(nome, sobrenome);
	printf("\n");
	printf("O seu nome completo é: %s", nome);
	printf("\n");
	return 0;
}
