#include <stdio.h>

int main() {
    //    LINHA e COLUNA
    int numero[5][5];
    int linha = 0, coluna = 0;

    //Comando para navegar pelas linhas
    for (linha = 0; linha < 5; linha++) {
        //Comando para navegar pelas colunas
        for (coluna = 0; coluna < 5; coluna++) {
            printf("Valor inteiro na linha %d, coluna %d: ", linha + 1, coluna + 1);
            scanf_s("%d", &numero[linha][coluna]);
        }
    }
    return (0);
}
